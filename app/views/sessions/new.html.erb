<h1>Login Page (under sessions)</h1>
<%= form_tag sessions_path do %>
<div class = "field">
<%= label_tag :email %>
<%= text_field_tag :email %>
</div>
<div class = "field">
<%= label_tag :password %>
<%= password_field_tag :password %>
</div>
<div class = "actions">
<%= submit_tag "Sign In" %>
</div>


<%end %>

<%= link_to 'Sign Up', new_user_path %>
